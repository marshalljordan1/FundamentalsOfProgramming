
public class OOP {

	public static void main(String[] args) {
		System.out.println("hello world");
		User u = new User();
		
		u.name = "Jordan";
		u.membership = "Premium";
		u.age = 27;
		
		User u2 = new User();
		u2.name = "Caleb";
		u2.membership = "Premium";
		
		System.out.println(u.name);
		System.out.println(u2.name);
		System.out.println(u.age);
	}

}
