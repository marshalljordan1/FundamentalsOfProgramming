package carChampionship;

public class Drivers extends Teams {
	
	private int height;
	private int weight;
	
	public Drivers(String teamName, int budget, String country, int height, int weight) {
		super(teamName, budget, country);
		this.height = height;
		this.weight = weight;
	}

	public int getHeight() {
		return height;
	}

	public void setHeight(int height) {
		this.height = height;
	}

	public int getWeight() {
		return weight;
	}

	public void setWeight(int weight) {
		this.weight = weight;
	}
}
